# Managed by puppet
[puppetlabs-products]
name=Puppet Labs Products El <%= scope.lookupvar('yum::params::puppetlabs_os_version') %> - $basearch
baseurl=http://yum.puppetlabs.com/el/<%= scope.lookupvar('yum::params::puppetlabs_os_version') %>/products/$basearch
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs
enabled=1
gpgcheck=1
priority=30

