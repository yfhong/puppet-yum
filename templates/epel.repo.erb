# Managed by puppet
[epel]
name=Extra Packages for Enterprise Linux <%= scope.lookupvar('yum::params::epel_os_version') %> - $basearch
baseurl=http://<%= scope.lookupvar('yum::epel_repo_mirror') %>/epel/<%= scope.lookupvar('yum::params::epel_os_version') %>/$basearch
enabled=1
gpgcheck=1
gpgkey=https://dl.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-<%= scope.lookupvar('yum::params::epel_os_version') %>
priority=50

