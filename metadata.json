{
  "name": "homer-yum",
  "version": "0.1.0",
  "author": "homer",
  "summary": "Puppet module for managing yum settings.",
  "license": "Apache 2.0",
  "source": "https://github.com/yfhong/puppet-yum.git",
  "project_page": "https://github.com/yfhong/puppet-yum",
  "issues_url": "https://github.com/yfhong/puppet-yum/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
